{
    "name": "mamluk/zilore-dns-sdk",
    "type": "library",
    "description": "A PHP SDK to interact with the Zilore API",
    "keywords": [
        "DNS",
        "Zilore",
        "Zilore DNS"
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.7",
        "illuminate/http": "^10.13",
        "symfony/yaml": "^6.3",
        "monolog/monolog": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Zilore\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    }
}
